{
  "name": "ummalqura-calendar",
  "title": "Umm al-Qura Comparative Calendar Dataset",
  "description": "A dataset of Umm al-Qura comparative calendar including Hijri and Gregorian dates for the years 1300-1500 AH",
  "homepage": "https://github.com/dralshehri/ummalqura-calendar",
  "licenses": [
    {
      "name": "PDDL-1.0",
      "path": "https://www.opendatacommons.org/licenses/pddl/1.0/",
      "title": "Open Data Commons Public Domain Dedication and Licence 1.0"
    }
  ],
  "resources": [
    {
      "path": "data/ummalqura-calendar.csv",
      "name": "ummalqura-calendar",
      "format": "csv",
      "mediatype": "text/csv",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "rjd",
            "type": "integer",
            "title": "Reduced Julian Day",
            "constraints": {
              "unique": true
            }
          },
          {
            "name": "hy",
            "type": "integer",
            "title": "Hijri lunar year"
          },
          {
            "name": "hm",
            "type": "integer",
            "title": "Hijri lunar month"
          },
          {
            "name": "hd",
            "type": "integer",
            "title": "Hijri lunar day"
          },
          {
            "name": "gy",
            "type": "integer",
            "title": "Gregorian year"
          },
          {
            "name": "gm",
            "type": "integer",
            "title": "Gregorian month"
          },
          {
            "name": "gd",
            "type": "integer",
            "title": "Gregorian day"
          }
        ],
        "missingValues": [
          ""
        ]
      }
    }
  ]
}
